{
  "seed": 42,
  "epochs": 10,
  "model_name": "roberta-base",
  "train_batch_size": 32,
  "valid_batch_size": 64,
  "learning_rate": 1e-4,
  "max_length": 128,
  "margin": 0.5,
  "use_gpu": true,
  "run_mode": "recurrent",
  "wandb": true,
  "preprocessing": [],
  "vocab_file": "res/vocab/naive.txt",
  "embedding_dimension": 300,
  "embedding_type": "glove",
  "dropout": 0.2,
  "output_features": 128,
  "training_set": {
    "path": "res/data/train.csv",
      "cols": ["comment_text"],
      "type": "weighted",
      "download": "https://www.dropbox.com/s/33ukaurgr2lrt33/train.csv?dl=0"
  },
  "test_set": {
      "path": "res/data/validation_data.csv",
      "cols": ["more_toxic", "less_toxic"],
      "type": "pairwise",
      "download": "https://www.dropbox.com/s/igolk06nnfxiera/validation_data.csv?dl=0"
  }
}
